	Controls.SprintingButton.Visible = false
	Controls.DesktopContainer.Visible = false
end
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = true
		ws = 27.1
		if ability == false then
			hum.WalkSpeed = 27.1
		end
	end
end)
uis.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = false
		ws = 12
		if ability == false then
			hum.WalkSpeed = 12

		end
	end
end)

local function onButton1Down()
	if id ~= M1Left and ability == false and mouse1 == false and LMB == false then 
		mouse1 = true
		LMB = true
		local Anim2
		local animation1 = game:GetObjects("rbxassetid://" .. M1Left)[1]
		animation1.Parent = char
		Anim2 = AnimHandler.new(char, animation1)
		Anim2:Play()
		Anim:Stop()
		id = M1Left
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		animation1:Destroy()
		mouse1 = false
	elseif  id ~= M1Right and ability == false and mouse1 == false and LMB == true then 
		mouse1 = true
		LMB = false
		local Anim2
		local animation1 = game:GetObjects("rbxassetid://" .. M1Right)[1]
		animation1.Parent = char
		Anim2 = AnimHandler.new(char, animation1)
		Anim2:Play()
		Anim:Stop()
		id = M1Right
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		animation1:Destroy()
		mouse1 = false
	end
end

mouse.Button1Down:Connect(onButton1Down)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.E and ability == false then
		ability = true
		hum.WalkSpeed = 0.1
		local targetPosition = mouse.Hit.p
		player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition) + Vector3.new(0,4,0)
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. teleport)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(1)
		id = teleport
		ability = false
		hum.WalkSpeed = ws
	end
end)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q and ability == false then
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. poisonedheart)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(1.4)
		hum.WalkSpeed = ws
		ability = false
		id = poisonedheart
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.R and ability == false and debounce == false then
		hum.JumpPower = 0
		debounce = true
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. DashStart)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(0.25)
		dash = true
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. Dashing)[1]
		Anim = AnimHandler.new(char, animation)
		Anim.Looped = true
		Anim:Play()
		local c = Instance.new("BodyVelocity")
		c.MaxForce = Vector3.new(1,0,1) * 30000
		c.Parent = char.HumanoidRootPart
		c.Name = "DashVelocity"
		local counter = 10 * 1 -- the amount of seconds
		for i = counter, 0, -1 do
			if dash == true then
				task.wait(.1)
				c.Velocity = char.HumanoidRootPart.CFrame.lookVector * 50
			else return
			end
		end
		if dash == true then
			wait(2)
			c:Destroy()
			ability = false
			id = Dashing
			hum.WalkSpeed = ws
			hum.JumpPower = 50
			debounce = false
		end
	end
end)
function onTouched(hit)
	if dash == true then
		if not hit or not hit.Parent then return end
		local human = hit.Parent:findFirstChild("Humanoid")
		if human and human:IsA("Humanoid") then
			dash = false
			char.HumanoidRootPart.DashVelocity:Destroy()
			Anim:Stop()
			animation = game:GetObjects("rbxassetid://" .. DashHit)[1]
			Anim = AnimHandler.new(char, animation)
			Anim:Play() 
			wait(0.8)
			hum.JumpPower = 50
			ability = false
			id = DashHit
			hum.WalkSpeed = ws
			wait(2)
			debounce = false
		elseif hit:IsA("Part") and hit.CanCollide == true then
			
			char.HumanoidRootPart.CanCollide = true
			dash = false
			char.HumanoidRootPart.DashVelocity:Destroy()
			Anim:Stop()
			animation = game:GetObjects("rbxassetid://" .. DashWall)[1]
			Anim = AnimHandler.new(char, animation)
			Anim:Play() 
			wait(1)
			hum.JumpPower = 50
			char.HumanoidRootPart.CanCollide = false
			ability = false
			id = DashHit
			hum.WalkSpeed = ws
			wait(2)
			debounce = false
		end
	end
end
char.Torso.Touched:connect(onTouched)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.G then
		if EmoteGui.Enabled == false then
			EmoteGui.Enabled = true
		elseif EmoteGui.Enabled == true then
			EmoteGui.Enabled = false
		end
	end
end)

