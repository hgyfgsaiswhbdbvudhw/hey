local eight 
local target
local walk = 84875197555156
local mouse1 = false
local i = 134330609256449
local sprinting1 = false
local ability = false
local run = 88906851775352
local uis = game:GetService("UserInputService")	
local walking = false
local M1Left = 129017582084012
local LMB = false
local M1Right = 98193316373691
local poisonedheart = 82126400149279
local teleport = 78764683268990
local Stunned = 107384292254187
local DashStart = 123923597484605
local Dashing = 118592585726163
local DashHit = 82985386722928
local DashWall = 116326154605240
local dash = false
local hurt = false
local sprinting = false
local ws = 12
local emote = false
local player =  game:GetService("Players").LocalPlayer
local mouse = player:GetMouse()
local debounce = false
local idle = true
local char = game.Players.LocalPlayer.Character
local hum: Humanoid = char:FindFirstChild("Humanoid")
local walksfx = Instance.new("Sound")
walksfx.SoundId = 174960816
walksfx.Parent = char.HumanoidRootPart
walksfx.PlaybackSpeed = 1
walksfx.PlaybackRegionsEnabled = true
walksfx.LoopRegion = NumberRange.new(0, 0.475)
walksfx.Volume = 5
walksfx.Looped = true
walksfx.Playing = false
local attackvel = Instance.new("VectorForce")
attackvel.Parent = char.HumanoidRootPart
attackvel.Attachment0 = char.HumanoidRootPart.RootAttachment
attackvel.Enabled = false
attackvel.Force = Vector3.new(0, 0, -15000)
hum.Animator:Destroy()
char.Animate:Destroy()
local animation = game:GetObjects("rbxassetid://" .. i)[1]
animation.Parent = char
id = i
Anim = AnimHandler.new(char, animation)
Anim:Play()
hum.WalkSpeed = ws
if uis.TouchEnabled == true then
	Mobile.Enabled = true
