-- Play Emote on press
one11.MouseButton1Click:Connect(function()
emote = true
local soundFileName = "Emote"
wf(soundFileName, one)
local music = Instance.new("Sound", game.Players.LocalPlayer.Character.HumanoidRootPart)
music.Volume = 1
music.Name = "Emote"
music.SoundId = getcustomasset(soundFileName)
music.Looped = true
music:Play()
Anim:Stop()
animation = game:GetObjects("rbxassetid://" .. one1.AnimID.Value)[1]
Anim = AnimHandler.new(char, animation)
Anim:Play()
EmoteGui.Enabled = false
end)
