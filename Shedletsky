local EmoteGui: ScreenGui = game:GetObjects("rbxassetid://70965231817963")[1]
EmoteGui.Parent = game.Players.LocalPlayer.PlayerGui
EmoteGui.Enabled = false
local Destroyed = false
local Mobile: ScreenGui = game:GetObjects("rbxassetid://77707674968679")[1]
Mobile.Parent = game.Players.LocalPlayer.PlayerGui
Mobile.Enabled = false
local Controls: ScreenGui = game:GetObjects("rbxassetid://102160674504380")[1]
Controls.Parent = game.Players.LocalPlayer.PlayerGui
Controls.Enabled = true
Controls.MobileContainer.Visible = false
Controls.Version.Text = "Version: Shedletsky 1.0"
local AnimHandler = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProudNamed/SuperLightning/refs/heads/main/AnimModule/MainModule"))()
local Anim
local id
local one
local two 
local three 
local four 
local five 
local six 
local seven 
local eight 
local walk = 108018357044094
local mouse1 = false
local i = 131082534135875
local sprinting1 = false
local ability = false
local run = 136252471123500
local uis = game:GetService("UserInputService")	
local walking = false
local hwalking = 132377038617766
local Slash = 116618003477002
local Eat = 95134003187089
local hurt = false
local hidle = 134624270247120
local hrun = 115946474977409
local sprinting = false
local ws = 12
local emote = false
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local mouse = player:GetMouse()
local idle = true
local char = game.Players.LocalPlayer.Character
local hum: Humanoid = char:FindFirstChild("Humanoid")
local Slashvel = Instance.new("VectorForce")
Slashvel.Parent = char.HumanoidRootPart
Slashvel.Attachment0 = char.HumanoidRootPart.RootAttachment
Slashvel.Enabled = false
Slashvel.Force = Vector3.new(0, 0, -15000)
hum.Animator:Destroy()
char.Animate:Destroy()
local animation = game:GetObjects("rbxassetid://" .. i)[1]
animation.Parent = char
id = i
Anim = AnimHandler.new(char, animation)
Anim:Play()
hum.WalkSpeed = ws
if uis.TouchEnabled == true then
	Mobile.Enabled = true
	Controls.MobileContainer.Visible = true
	Controls.SprintingButton.Visible = true
	Controls.DesktopContainer.Visible = false
	Controls.MobileContainer.Punch.Visible = false
elseif uis.TouchEnabled == false then
	Mobile.Enabled = false
	Controls.DesktopContainer.Block.Image = "rbxassetid://85282571177550"
	Controls.DesktopContainer.Block.Clipping.Top.Image = "rbxassetid://85282571177550"
	Controls.DesktopContainer.Charge.Image = "rbxassetid://87451970500470"
	Controls.DesktopContainer.Charge.Clipping.Top.Image = "rbxassetid://87451970500470"
	Controls.DesktopContainer.Punch.Image = "rbxassetid://0"
	Controls.DesktopContainer.Punch.Clipping.Top.Image = "rbxassetid://0"
	Controls.DesktopContainer.Block.AbilityName.Text = "Slash"
	Controls.DesktopContainer.Charge.AbilityName.Text = "Eat"
	Controls.DesktopContainer.Punch.AbilityName.Text = ""
	Controls.DesktopContainer.Punch.Visible = false
	Controls.MobileContainer.Visible = false
	Controls.SprintingButton.Visible = false
	Controls.DesktopContainer.Visible = true
end
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = true
		ws = 24
		if ability == false then
			hum.WalkSpeed = 24
		end
	end
end)
uis.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = false
		ws = 12
		if ability == false then
			hum.WalkSpeed = 12
		end
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.E and ability == false then
		ability = true
		hum.WalkSpeed = 4
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. Eat)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(2.8)
		ability = false
		id = Eat
		hum.WalkSpeed = ws
	end
end)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q and ability == false then
		ability = true
		hum.WalkSpeed = 2
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. Slash)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(.5)
		Slashvel.Enabled = true
		wait(.1)
		Slashvel.Enabled = false
		wait(.2)
		ability = false
		id = Slash
		hum.WalkSpeed = ws
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.T then
		if hurt == false then
			hurt = true
		elseif hurt == true then
			hurt = false
		end
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.G then
		if EmoteGui.Enabled == false then
			EmoteGui.Enabled = true
		elseif EmoteGui.Enabled == true then
			EmoteGui.Enabled = false
		end
	end
end)

